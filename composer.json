{
  "name": "infinum/eightshift-forms",
  "type": "eightshift-forms",
  "description": "This is Eightshift_Forms plugin used for contact forms.",
  "keywords": [
    "plugin",
    "forms",
    "WordPress"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "infinum/eightshift-libs": "^0.8.0",
    "infinum/eightshift-blocks": "^1.0.5"
  },
  "require-dev": {
    "infinum/coding-standards-wp": "0.4.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.5"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "process-timeout": 2000
  },
  "scripts": {
    "check-cs": "@php ./vendor/bin/phpcs",
    "fix-cs": "@php ./vendor/bin/phpcbf"
  }
}
